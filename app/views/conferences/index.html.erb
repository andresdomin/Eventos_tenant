<h1>Listing conferences</h1>

<table>
  <tr>
    <th>Nombre</th>
    <th>Floor</th>
    <th>Description</th>
    <th>Longitude</th>
    <th>Latitude</th>
    <th>Tenant</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @conferences.each do |conference| %>
  <tr>
    <td><%= conference.nombre %></td>
    <td><%= conference.floor %></td>
    <td><%= conference.description %></td>
    <td><%= conference.longitude %></td>
    <td><%= conference.latitude %></td>
    <td><%= conference.tenant %></td>
    <td><%= link_to 'Show', conference %></td>
    <td><%= link_to 'Edit', edit_conference_path(conference) %></td>
    <td><%= link_to 'Destroy', conference, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Conference', new_conference_path %>
